{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "moduleResolution": "node",
    "strict": true,
    "allowJs": true,
    "removeComments": true,
    "allowUnreachableCode": false,
    "alwaysStrict": true,
    "noImplicitThis": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "esModuleInterop": true
  }
}
